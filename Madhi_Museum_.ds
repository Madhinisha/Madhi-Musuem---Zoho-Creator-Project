			Validate_Booking_date as "Validate Booking date"
			{
				type =  form
				form = Ticket_Booking
				record event = on add or edit

				on user input of Booking_Date
				{
					actions 
					{
						custom deluge script
						(
											if(input.Event_Name != null)
										{
											if(input.Booking_Date != null)
											{
												if(input.Booking_Date < input.Start_Date || input.Booking_Date > input.End_Date)
												{
													alert "Choose the Date between start date and end date";
													input.Booking_Date = zoho.currentdate;
												}
												else if(input.Booking_Date < zoho.currentdate)
												{
													alert "Date is not available";
													input.Booking_Date = zoho.currentdate;
												}
											}
										}
						)
					}
					actions 
					{
						custom deluge script
						(
											selectedDate = input.Booking_Date;
										selectedevent = input.Event_Name;
										fetch_event = Events[ID == input.Event_Name];
										ticketReport = Ticket_Booking[Event_Name == selectedevent && Booking_Date == selectedDate] sort by Added_Time desc;
										if(ticketReport != null && fetch_event.ID == input.Event_Name)
										{
											totalVisitors = ticketReport.No_of_Adult_Visitors + ticketReport.No_of_Child_Visitors;
											totalTicketsPerDay = ticketReport.Available_Ticket;
											availableTickets = totalTicketsPerDay - totalVisitors;
											input.Available_Ticket = availableTickets;
										}
										if(input.Booking_Date != ticketReport.Booking_Date)
										{
											input.Available_Ticket = input.Total_Tickets_per_day;
										}
										if(input.Available_Ticket = 0)
										{
											alert "Tickets are already booked for this date, Please choose another date";
											input.Booking_Date = null;
										}
						)
					}
				}

			}
